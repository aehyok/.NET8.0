[Unit]
Description=aehyok-ncdp
After=network-online.target
Wants=network-online.target

[Service]
# modify when deploy in prod env
# User=dvs
# Group=dvs

Type=simple
ExecStart=/root/.dotnet/dotnet /usr/local/aehyok/aehyok-ncdp/aehyok.NCDP.Api.dll
WorkingDirectory=/usr/local/aehyok/aehyok-ncdp

Restart=always
RestartSec=1
StartLimitInterval=0

[Install]
WantedBy=multi-user.target
